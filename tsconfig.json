{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "es2016",
      "esnext"
    ],
    "outDir": "",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": [
    "src/**/**.test.ts"
  ],
  "include": [
    "src/**/*"
  ]
}
